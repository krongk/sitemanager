<%= simple_form_for(@sms_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :sms_tmp %>
    <%= f.input :resource %>
    <%= f.input :resource_id %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
